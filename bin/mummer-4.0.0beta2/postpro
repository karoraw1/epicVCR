#! /bin/sh

# postpro - temporary wrapper script for .libs/postpro
# Generated by libtool (GNU libtool) 2.4.6.40-6ca5
#
# The postpro program cannot be directly executed until all the libtool
# libraries that it depends on are installed.
#
# This wrapper script should never be moved out of the build directory.
# If it is, it will not operate correctly.

# Sed substitution that helps us do robust quoting.  It backslashifies
# metacharacters that are still active within double-quoted strings.
sed_quote_subst='s|\([`"$\\]\)|\\\1|g'

# Be Bourne compatible
if test -n "${ZSH_VERSION+set}" && (emulate sh) >/dev/null 2>&1; then
  emulate sh
  NULLCMD=:
  # Zsh 3.x and 4.x performs word splitting on ${1+"$@"}, which
  # is contrary to our usage.  Disable this feature.
  alias -g '${1+"$@"}'='"$@"'
  setopt NO_GLOB_SUBST
else
  case `(set -o) 2>/dev/null` in *posix*) set -o posix;; esac
fi
BIN_SH=xpg4; export BIN_SH # for Tru64
DUALCASE=1; export DUALCASE # for MKS sh

# The HP-UX ksh and POSIX shell print the target directory to stdout
# if CDPATH is set.
(unset CDPATH) >/dev/null 2>&1 && unset CDPATH

relink_command="(cd /home-3/karoraw1@jhu.edu/scratch/THRASH_LIBS/mummer-4.0.0beta2; LIBRARY_PATH=/cm/shared/apps/slurm/current/lib/slurm:/cm/shared/apps/slurm/current/lib:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/compiler/lib/intel64:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/mkl/lib/intel64:/cm/shared/apps/sqlite3/3.15.0/lib:/cm/shared/apps/hdf5/1.8.17/lib:/cm/shared/apps/netcdf/intel/4.3.3.1/lib:/cm/shared/apps/hdf5_18/1.8.15/lib:/cm/shared/apps/zlib/1.2.8/lib:/cm/shared/apps/netcdf-fortran/intel/4.4.2/lib; export LIBRARY_PATH; { test -z \"\${COMPILER_PATH+set}\" || unset COMPILER_PATH || { COMPILER_PATH=; export COMPILER_PATH; }; }; { test -z \"\${GCC_EXEC_PREFIX+set}\" || unset GCC_EXEC_PREFIX || { GCC_EXEC_PREFIX=; export GCC_EXEC_PREFIX; }; }; LD_RUN_PATH=/cm/shared/apps/Intel/openmpi/1.8.4/lib:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/compiler/lib/intel64:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/lib/mkl/lib/intel64:/cm/shared/apps/hdf5_18/1.8.15/lib; export LD_RUN_PATH; LD_LIBRARY_PATH=/cm/shared/apps/slurm/current/lib/slurm:/cm/shared/apps/slurm/current/lib:/cm/shared/apps/gcc/4.8.2/lib64:/cm/shared/apps/gcc/4.8.2/lib:/cm/shared/apps/perl/5.22.1/lib/5.22.1:/cm/shared/apps/xz-libs/5.2.2/lib:/cm/shared/apps/perl/5.22.1/lib/5.22.1:/cm/shared/apps/java/jdk1.6.0_45/lib:/cm/shared/apps/Intel/openmpi/1.8.4/lib:/cm/shared/apps/binutils/2.25/src/lib:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/compiler/lib/intel64:/cm/shared/apps/parallel_studio_xe_2015_update2/composer_xe_2015.2.164/mkl/lib/intel64:/cm/shared/apps/gsl/1.16/lib:/cm/shared/apps/sqlite3/3.15.0/lib:/cm/shared/apps/hdf5/1.8.17/lib:/cm/shared/apps/blast/2.2.30/lib64:/cm/shared/apps/ant/1.9.5/lib:/cm/shared/apps/netcdf/intel/4.3.3.1/lib:/cm/shared/apps/hdf5_18/1.8.15/lib:/cm/shared/apps/zlib/1.2.8/lib:/cm/shared/apps/netcdf/gcc/64/4.3.1.1/lib:/cm/shared/apps/netcdf-f